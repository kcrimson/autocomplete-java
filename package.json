{
  "name": "autocomplete-java",
  "main": "./lib/atomAutocompletePackage",

  "version": "1.2.5",
  "description": "Autocomplete-plus provider for Java",
  "repository": "https://github.com/keskiju/autocomplete-java",
  "license": "MIT",
  "keywords": [
    "java",
    "autocomplete-plus",
    "autocomplete",
    "import"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "autocomplete-java:organize-imports",
      "autocomplete-java:refresh-project",
      "autocomplete-java:full-refresh"
    ]
  },
  "activationHooks": [
    "language-java:grammar-used"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "dependencies": {
    "triejs": "0.1.5",
    "walk": "2.3.9",
    "async": "^1.5.2",
    "google-protobuf": "^3.0.0-alpha.5",
    "grpc": "^1.0.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.4.3",
    "eslint-config-airbnb": "^0.1.0"
  }
}
